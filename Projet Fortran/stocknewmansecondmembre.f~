      SUBROUTINE NEWMANSECONDMEMBRE(N,M,VECTEUR_TRIANGLE,
     G VECTEUR_BOUND,CONDNE,
     J VECTEURX,VECTEURY,VECTEUR_SECONDMEMBRE)
      IMPLICIT NONE
      INTEGER  I,N,M,POS,VECTEUR_TRIANGLE((N+1)*(M+1)),
     A VECTEUR_BOUND(2*(M+N+1))
      REAL  VECTEUR_SECONDMEMBRE((N+1)*(M+1)),FJ(3),CONDNE(4)
     F ,VECTEURX(N+1),VECTEURY(M+1),BOUNDX,BOUNDY


      POS = N+2
      DO 2 I=1,M

      VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS)) = 
     L VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS))+ CONDNE(2)*
     L (1./2.*(VECTEURY(VECTEUR_BOUND(POS+1))-
     L VECTEURY(VECTEUR_BOUND(POS))))


       VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS+1)) = 
     L VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS+1))+ CONDNE(2)*
     L (1./2.*(VECTEURY(VECTEUR_BOUND(POS+1))-
     L VECTEURY(VECTEUR_BOUND(POS))))
      
      POS = POS +1 
2     CONTINUE

      POS = M+N+3
      DO 3 I=1,N  
      VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS)) = 
     L VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS))+ CONDNE(3)*
     L (1./2.*(VECTEURX(VECTEUR_BOUND(POS+1))-
     L VECTEURX(VECTEUR_BOUND(POS))))
      
       VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS+1)) = 
     L VECTEUR_SECONDMEMBRE(VECTEUR_BOUND(POS+1))+ CONDNE(3)*
     L (1./2.*(VECTEURX(VECTEUR_BOUND(POS+1))-
     L VECTEURX(VECTEUR_BOUND(POS))))
  

      POS = POS + 1
3     CONTINUE



      END
