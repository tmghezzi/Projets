      SUBROUTINE CHOLESKY(N,M,SIZEMAX,VECTEURMIN,VECTEUR_DIAGONAL,
     L VECTEUR_CHOLESKY,VECTEUR_SECONDMEMBRE,VECTEUR_Y)
      IMPLICIT NONE
      INTEGER  I,J,K,N,M,SIZEMAX,POSITIONVECT,POSITIONVECT1,
     L POSITIONVECT2,POSITIONVECT3,K1,VECTEUR_MIN((N+1)*(M+1)),
     L VECTEUR_DIAGONAL((N+1)*(M+1))

      REAL VECTEUR_STOCKAGE(SIZEMAX),VECTEUR_CHOLESKY(SIZEMAX),
     L  SUMA,SUMB,SUMA2,SUMB2
      SUMA = 0
      SUMB = 0
      SUMB2= 0
      SUMA2= 0
      DO 1 I=1,(N+1)*(M+1)
      SUMA = VECTEUR_SECONDMEMBRE(I)
      SUMB = 0
           DO 2 K=VECTEURMIN(I),I-1
           POSITIONVECT = VECTEUR_DIAGONAL(I)-I+K
           SUMB =  VECTEUR_CHOLESKY(POSITIONVECT)*VECTEUR_Y(K)
2          CONTINUE 
1     CONTINUE
  

      END
