      SUBROUTINE SECONDMEMBRE(N,M,ITERATOR,FJ,VECTEUR_TRIANGLE,
     G VECTEUR_BOUND,CONDNE,VECTEUR_SECONDMEMBRE)
      IMPLICIT NONE
      INTEGER  I,N,M,POS,ITERATOR,VECTEUR_TRIANGLE((N+1)*(M+1)),
     A VECTEUR_BOUND(2*(M+N+1))
      REAL  VECTEUR_SECONDMEMBRE((N+1)*(M+1)),FJ(3),CONDNE(4)
    
      POS = VECTEUR_TRIANGLE(ITERATOR)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS) + FJ(1)
      POS = VECTEUR_TRIANGLE(ITERATOR+1)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS) + FJ(2)  
      POS = VECTEUR_TRIANGLE(ITERATOR+2)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS) + FJ(3)
      DO 1 I=1,N+1
      WRITE(*,*) 'B1:',VECTEUR_BOUND(I)
      POS = VECTEUR_BOUND(I)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS)+CONDNE(1)
1     CONTINUE
      DO 2 I=N+2,M+N+2
      WRITE(*,*) 'B2:',VECTEUR_BOUND(I)
      POS = VECTEUR_BOUND(I)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS)+CONDNE(2)
2     CONTINUE
      DO 3 I=M+N+3,2*M+N+4
      WRITE(*,*) 'B3:',VECTEUR_BOUND(I)
      POS = VECTEUR_BOUND(I)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS)+CONDNE(3)
3     CONTINUE
      DO 4 I=2*M+N+5,2*(N+M+2)
      WRITE(*,*) 'B4:',VECTEUR_BOUND(I)
      POS = VECTEUR_BOUND(I)
      VECTEUR_SECONDMEMBRE(POS) = VECTEUR_SECONDMEMBRE(POS)+CONDNE(4)
4     CONTINUE
      END
